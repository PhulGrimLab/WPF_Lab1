<Window x:Class="AsyncAwaitDeepSeek1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsyncAwaitDeepSeek1"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="400">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <!-- 진행률 표시 ProgressBar -->
        <ProgressBar x:Name="progressBar" Width="300" Height="20" Minimum="0" Maximum="100" />

        <!-- 작업 시작 Button -->
        <Button x:Name="startButton" Content="Start Task" Width="100" Height="30" Margin="0,20,0,0" Click="StartButton_Click" />
    </StackPanel>
</Window>
